{
  "name": "emoji-domain",
  "version": "0.0.0",
  "description": "Website for my emoji domain",
  "main": "./src/index.html",
  "scripts": {
    "dev": "parcel ./src/index.html/",
    "build": "parcel build ./src/index.html/ --public-url ./",
    "lint": "eslint --ext .js ./src",
    "lint-fix": "eslint --fix --ext .js ./src"
  },
  "author": "SUBHUMAN <sebi.wachter@onlin.de>",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb/base"
    ],
    "env": {
      "browser": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "off"
    },
    "settings": {
      "import/resolver": {
        "parcel": {
          "rootDir": "./src"
        }
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "dependencies": {
    "three": "^0.108.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-parcel": "^1.10.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "parcel-bundler": "^1.12.3"
  }
}
